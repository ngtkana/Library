			"\ta -= b;",
			"\tif (a < 0) a += md;",
			"}",
			"inline int mul(int a, int b) {",
			"\treturn (int)((long long)a * b % md);",
			"}",
			"inline int power(int a, long long b) {",
			"\tint res = 1;",
			"\twhile (b > 0) {",
			"\t\tif (b & 1) {",
			"\t\t\tres = mul(res, a);",
			"\t\t\tb--;",
			"\t\t}",
			"\t\ta = mul(a, a);",
			"\t\tb >>= 1;",
			"\t}",
			"\treturn res;",
			"}",
			"inline int inv(int a) {",
			"\ta %= md;",
			"\tif(a < 0) a += md;",
			"\tint b = md, u = 0, v = 1;",
			"\twhile (a) {",
			"\t\tint t = b / a;",
			"\t\tb -= a * t; swap(a, b);",
			"\t\tu -= v * t; swap(u, v);",
			"\t}",
			"\tassert(b == 1);",
			"\tif(u < 0) u += md;",
			"\treturn u;",
			"}"
